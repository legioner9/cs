    class Program
    {
        static void Main()
        {
            byte[] num_arr = { 3, 5, 7 };
            Console.WriteLine("Result: " + SumArr(num_arr));

        }

        public static byte SumArr(byte[] arr_arg)
        {
            byte sum = 0;
            foreach (byte el in arr_arg)
            {
                sum += el;
            }
            return sum;
        }
    }
